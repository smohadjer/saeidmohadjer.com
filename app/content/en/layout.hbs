<!-- {{this.pageId}} -->
<!doctype html>
<html class="no-js" lang="en" {{#if (eq this.pageId 'index')}}class="home"{{/if}} >
	<head>
    {{> en/partials/meta }}
    {{> en/partials/styles }}
	</head>
	<body>
    {{#if (eq this.pageId 'service') }}
	    {{> en/partials/header pageId=this.pageId }}
    {{/if}}
{{> content }}
    {{#unless (eq this.pageId 'index') }}
        {{> en/partials/footer }}
        {{> en/partials/scripts }}
    {{/unless}}
	</body>
</html>
